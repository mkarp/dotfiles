alias aliases="$EDITOR $DOT/aliases"
alias bash_profile="$EDITOR ~/.bash_profile && refreshenv"
alias create_macos_usb="sudo /Applications/Install\ macOS\ Sierra.app/Contents/Resources/createinstallmedia --volume /Volumes/MISHA --applicationpath /Applications/Install\ macOS\ Sierra.app --nointeraction"
alias daily="$EDITOR $HOME/Daily/`date +'%Y'`/`date +'%m'`/`date +'%d'`.txt"
alias dot="cd $DOT"
alias dot_backup="dot; git add .; git stash; git up; git stash pop; git wip; git pu; cd -"
alias dot_update="dot; git up; cd -"
alias gh="open \$(ghurl)"
alias ghpr="git push --set-upstream origin \$(git symbolic-ref --short HEAD) && open \$(ghprurl)"
alias ghprurl="echo \"\$(ghurl)/compare/\$(git symbolic-ref --short HEAD)\""
alias ghurl="echo \"\$(git config --get remote.origin.url | sed -e 's/git@github.com:/https:\/\/github.com\//' | sed -e 's/\.git//')\""
alias pbsort="pbpaste | sort -u | pbcopy"
alias reset_dns_cache="sudo killall -HUP mDNSResponder"
alias rm_dashboard="defaults write com.apple.dashboard mcx-disabled -boolean YES; killall Dock"
alias rm_derived_data="kill_xcode; rm -rf $DD; open /Applications/Xcode.app"
alias rm_desktop_icons="defaults write com.apple.finder CreateDesktop false; killall Finder"
alias rm_ds_store="find . -name .DS_Store -delete"
alias shell_secrets="$EDITOR $SHELL_SECRETS"
alias sleep="pmset sleepnow"
alias wrk="cd $WRK"
alias zshrc="$EDITOR ~/.zshrc && refreshenv"
